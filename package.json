    "next": "^14.1.0",
    "next-mdx-remote": "^4.4.1",
    "next-themes": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-to-print": "^2.14.7",
    "rehype-pretty-code": "^0.13.0",
