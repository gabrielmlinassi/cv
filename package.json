    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-slug": "^6.0.0",
    "shikiji": "^0.10.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "typescript-plugin-css-modules": "^3.4.0"
